MONGO_ROOT_USERNAME=mongo root username # Mongo root username
MONGO_ROOT_PASSWORD=mongo root password # Mongo root password
MONGO_HOST=mongo host # Mongo host; Default: mongo
MONGO_PORT=mongo port # Mongo port; Default: 27017
ENCRYPTION_KEY= encryption key # Generated by minha-lista-de-compras/utils/new_fernet_key.py
API_TOKEN= api token # Generated by minha-lista-de-compras/utils/new_api_token.py
SECRET_KEY_CSRF=Secret key CSRF # minha-lista-de-compras/utils/generate_csrf_token.py
